{
  "name": "node-app-for-api",
  "version": "1.0.0",
  "description": "Node and react app used to make API calls",
  "author": {
    "name": "Conor Carey"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev:server": "cd client && npm build && cd .. && npm start",
    "start": "node server.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react-moment": "^0.9.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
